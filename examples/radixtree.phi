radix_tree   :: union { *radix_branch|*radix_leaf }
radix_leaf   :: struct { label: text, value: any }
radix_branch :: struct { label: text, edges: [..]radix_tree }

walkRadix :: (tree radix_tree, walkFunc (*radix_leaf)) {
  if tree.type == *radix_leaf {
    walkFunc(tree)
  } else {
    for edge in tree.edges:
      walkRadix(edge, func)
  }
}
